      <TestGroup ID="4.7" Revision="0" Description="CPOW Test." Selectable="false" CancelNotPassed="true" TestMeasurementIDs="TM_06_00|TM_06_01|TM_06_02|TM_06_03|TM_06_04|TM_06_05|TM_06_06|TM_06_07|TM_06_08" />
